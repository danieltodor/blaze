{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/bin/blaze",
            // "args": ["bash", "--init"],
            "args": ["bash", "--prompt", "-s", "1669573672.173649637", "-f", "1669573895.264739827", "-e", "1", "-b", "0;0;0", "-p", "ls"],
            "cwd": "${workspaceFolder}",
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "make build project",
            "presentation": {
                "group": "project",
                "order": 1
            }
        }
    ]
}
